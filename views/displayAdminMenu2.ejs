<body class="p">
<!-- Testing something for the updating manga feature ~ Onyi -->
 
  <h1 class="jumbotron"></h1>
  <div class="placeholder">
    <div class="card">
      <% rows.forEach( function(row) { %>
        <div class="card-cont">
        [<a href="/updateManga?mangaID=<%= row.mangaID %>">Update</a>]

        [<a class="deleteUser" href="#" id="<%=row.mangaID%>">Delete</a>]

        <span>
        <%=row.title%> <%=row.price%>
        </span>
        <br>
        </div>
      <% }) %>
    </div>
  </div> 
  
