<body class="p">
  <!-- Moved the Login porition into the Login Route. ~~Onyi  --> 
  <h1 class="jumbotron">Account</h1>

  <div class="placeholder">
  <div class="cardLog text-center">
  <div class="card-header" id="cls">Header</div>
    <div class="card-body" id="cls">
      <div class="loginSection">
        <form method="POST">
          
          <input type="text" name="username" id="username" placeholder="Enter Username">
          <br>
          <input type="password" name="password" id="password" placeholder="Enter Password">
          <br><br>

        
          <button id="loginButt">Login</button>

          <div class="errC">
            <%=errMsg%>
          </div>

        </form>

      </div> 
    </div>
    <div class="card-footer" id="cls">
      <!-- Create Account -->
      <div class="createAccount">
        <form action="/addAccount">
          <button id="createAccButt">Create</button>
        </form>

      </div>
      <br>
      <div>
        <form action="/">
          <button id="return">Home</button>
        </form>
      </div>

    </div>
  </div>

  </div>

